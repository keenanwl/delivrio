{
  "name": "delivery",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json --port 4401",
    "start:returns": "ng serve return-portal --proxy-config proxy.conf.json --port 4403",
    "build": "ng build -c production",
    "build:returns": "ng build return-portal -c production  --output-hashing none",
    "test": "ng test",
    "lint": "ng lint --fix",
    "gen:graphql": "graphql-codegen",
    "e2e": "cypress run",
    "e2e:open": "npm run-script e2e:start-server && wait-on -v http-get://localhost:8080/api/health-check && cypress open",
    "e2e:start-server": "./cypress/start_server.sh",
    "e2e:stop-server": "./cypress/stop_server.sh"
  },
  "private": true,
  "dependencies": {
